                    </Viewbox>
                    <Viewbox Grid.Row="1" Grid.RowSpan="2">
                        <Label Name="TEMP09VAL" Content="ND"/>
                    </Viewbox>
                </Grid>
                <Grid Grid.Row="1" Grid.Column="4">
                    <Grid.RowDefinitions>
