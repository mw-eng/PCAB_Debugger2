                    </Viewbox>
                    <Viewbox Grid.Row="1" Grid.RowSpan="2">
                        <Label Name="TEMP04VAL" Content="ND"/>
                    </Viewbox>
                </Grid>
                <Grid Grid.Row="0" Grid.Column="4">
