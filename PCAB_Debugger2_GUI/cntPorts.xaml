                    <ComboBox Grid.Column="1" x:Name="ComboBox_ALL_DSA" ItemsSource="{StaticResource DSA_List}" SelectedIndex="0" Margin="5" IsEnabled="False" SelectionChanged="ComboBox_ALL_DSA_SelectionChanged"/>
                </Grid>
            </Viewbox>
            <Viewbox Grid.Column="2">
